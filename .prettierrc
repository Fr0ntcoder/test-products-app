{
	"trailingComma": "none",
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^@/api/(.*)$",
		"^@/app/(.*)$",
		"^@/components/(.*)$",
		"^@/config/(.*)$",
		"^@/utils/(.*)$",
		"^../(.*)$",
		"^./(?!.*\\.module\\.scss$).*$",
		"^./.*\\.module\\.scss$",
		"(.scss)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
